@article{van_neerven_dimensional_1986,
	title = {Dimensional regularization of mass and infrared singularities in two-loop on-shell vertex functions},
	volume = {268},
	issn = {0550-3213},
	url = {http://www.sciencedirect.com/science/article/pii/0550321386901653},
	doi = {10.1016/0550-3213(86)90165-3},
	abstract = {We show how massless two-loop on-shell vertex functions can be calculated in a very elegant way if mass and infrared singularities are regularized by n-dimensional regularization. Using dispersion methods one is able to express the Feynman integrals in a product of gamma functions. As an application of our techniques we have calculated a two-loop on-shell form factor. Its infrared behaviour will be compared with the predictions of some resummation formulae which have been derived in the literature.},
	number = {2},
	urldate = {2015-11-13},
	journal = {Nuclear Physics B},
	author = {Van Neerven, W. L.},
	month = may,
	year = {1986},
