@article{laenen_complete_1993,
	title = {Complete {O}(α{S}) corrections to heavy-flavour structure functions in electroproduction},
	volume = {392},
	issn = {0550-3213},
	url = {http://www.sciencedirect.com/science/article/pii/055032139390201Y},
	doi = {10.1016/0550-3213(93)90201-Y},
	abstract = {The complete O(αS) QCD corrections to the virtual-photon cross sections for heavy-flavour production in deep-elastic electron-proton scattering are presented. These results are then used to calculate the O(αS) corrections to the heavy-flavour contributions to the structure functions F2(x, Q2, m2) and FL(x, Q2, m2). We examine these corrections in the x and Q2 range appropriate for c- and b-quark production at HERA. Our results are also compared with the O(αS) plus O(αS2) corrections for massless quarks.},
	number = {1},
	urldate = {2015-10-16},
	journal = {Nuclear Physics B},
	author = {Laenen, E. and Riemersma, S. and Smith, J. and van Neerven, W. L.},
	month = mar,
	year = {1993},
	keywords = {Electroproduction, LO},
	pages = {162--228},
	file = {Nucl. Phys. B 392-162.pdf:/home/Felix/Physik/PhD/ref/Nucl. Phys. B 392-162.pdf:application/pdf}
}

@article{smith_qcd_1992,
	title = {{QCD} corrections to heavy flavour photoproduction and electroproduction},
	volume = {374},
	issn = {0550-3213},
	url = {http://www.sciencedirect.com/science/article/pii/055032139290476R},
	doi = {10.1016/0550-3213(92)90476-R},
	abstract = {We investigate the QCD corrections to the total cross sections and single-particle inclusive differential distributions for γ + p → Q(Q) + X and e + p → e + Q(Q) + X. In the latter reaction we assume that the exchanged photon is almost real (Weizsäcker-Williams approximation). This requires the calculation of the O(αs) corrections to the photon-parton reaction γ + g → Q + Q including the virtual gluon contributions and the soft and hard gluon radiation in the reaction γ + g → Q + Q + g. The contributions from γ + q(q) → Q + Q + q(q) are also calculated. We give exact results for the O(α2αS2) total cross sections and single particle inclusive differential distribution for b-quark production at HERA. Results for b- and t- production at future ep colliders (the LEP∗LHC option at CERN) are also presented.},
	number = {1},
	urldate = {2015-10-23},
	journal = {Nuclear Physics B},
	author = {Smith, J. and van Neerven, W. L.},
	month = apr,
	year = {1992},
	pages = {36--82},
	file = {Nucl. Phys. B 374-36.pdf:/home/Felix/Physik/PhD/ref/Nucl. Phys. B 374-36.pdf:application/pdf}
}

